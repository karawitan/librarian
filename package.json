{
  "name": "librarian",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start-old": "ng serve",
    "build-old": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "cd api && npm run api",
    "api-sync": "cd api-sync && npm run api-sync",
    "start": "cp -f ./src/environments/client.config.dev.js ./src/environments/client.config.js; ng serve --port 4201 --host=0.0.0.0",
    "build": "cp -f ./src/environments/client.config.prod.js ./src/environments/client.config.js; ng build",
    "build:prod": "cp -f ./src/environments/client.config.prod.js ./src/environments/client.config.js; ng build --prod",
    "copy": "scp -rp ./dist/html/ datirium-art:/meteor/scidap-ui/; rm -rf ./dist",
    "client-bundle": "rm -f ./node_modules/meteor-client.js && meteor-client bundle -s api && mv ./node_modules/meteor-client.js ./",
    "postinstall": "test -f ./meteor-client.js || npm run client-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.2",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "^9.1.2",
    "@angular/compiler": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.1.2",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "^9.1.2",
    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@clr/angular": "^3.1.0",
    "@clr/core": "^3.1.0",
    "@clr/icons": "^3.1.0",
    "@clr/ui": "^3.1.0",
    "@syncfusion/ej2-angular-buttons": "^18.1.43",
    "@syncfusion/ej2-angular-filemanager": "^18.1.42",
    "@syncfusion/ej2-angular-pdfviewer": "^18.1.44",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "meteor-client-bundler": "^0.6.0",
    "moment": "^2.24.0",
    "ng-circle-progress": "^1.4.1",
    "ngx-extended-pdf-viewer": "^3.2.0",
    "ngx-moment": "^3.5.0",
    "ngx-pipes": "^2.7.3",
    "rxjs": "^6.5.5",
    "screenfull": "^5.0.2",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
