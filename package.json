{
  "name": "bookstorage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-old": "ng serve",
    "build-old": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "cd api && npm run api",
    "api-sync": "cd api-sync && npm run api-sync",
    "start": "cp -f ./src/environments/client.config.dev.js ./src/environments/client.config.js; ng serve --port 4201",
    "build": "cp -f ./src/environments/client.config.prod.js ./src/environments/client.config.js; ng build",
    "build:prod": "cp -f ./src/environments/client.config.prod.js ./src/environments/client.config.js; ng build --prod",
    "copy": "scp -rp ./dist/html/ datirium-art:/meteor/scidap-ui/; rm -rf ./dist",
    "client-bundle": "rm -f ./node_modules/meteor-client.js && meteor-client bundle -s api && mv ./node_modules/meteor-client.js ./",
    "postinstall": "test -f ./meteor-client.js || npm run client-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@clr/angular": "^3.0.0",
    "@clr/core": "^3.0.0",
    "@clr/icons": "^3.0.0",
    "@clr/ui": "^3.0.0",
    "@syncfusion/ej2-angular-buttons": "^17.4.51",
    "@syncfusion/ej2-angular-filemanager": "^17.4.55",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "meteor-client-bundler": "^0.6.0",
    "moment": "^2.24.0",
    "ng-circle-progress": "^1.4.1",
    "ngx-extended-pdf-viewer": "^2.1.2",
    "ngx-moment": "^3.4.0",
    "ngx-pipes": "^2.5.6",
    "rxjs": "~6.5.4",
    "screenfull": "^5.0.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/meteor": "^1.4.41",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  },
  "depNotNeed": {
